GROQ_API_KEY=gsk_your_groq_key_here

# Optional: OpenAI (if you have a valid key)
# OPENAI_API_KEY=sk-proj-your-openai-key-here

# Server
PORT=8000

# CORS - Allowed Origins (comma-separated)
ALLOWED_ORIGINS=https://checkbhai.vercel.app
